<DragDropContext onDragEnd={this.onFaqDragEnd} key={index}>
        <Droppable droppableId="droppable">
          {(provided, snapshot) => (
            <div
              className="textFit"
              {...provided.droppableProps}
              ref={provided.innerRef}
              style={getFaqListStyle(snapshot.isDraggingOver)}
            >
              {this.state.faqList.map((faq, index) => (
                <Draggable key={faq.id} draggableId={faq.id} index={index}>
                  {(provided, snapshot) => (
                    <div
                      ref={provided.innerRef}
                      {...provided.draggableProps}
                      {...provided.dragHandleProps}
                      style={getFaqStyle(
                        snapshot.isDragging,
                        provided.draggableProps.style
                      )}
                    >
                      <Card key={index} className="col-md textFit">
                        <a
                          key={index}
                          className="btn toggler "
                          id={"toggler" + index}
                        >
                          <div className="faqQandAStyle">{faq.question}</div>
                        </a>
                        <UncontrolledCollapse toggler={"#toggler" + index}>
                          <div>
                            <div className="faqQandAStyle">{faq.answer}</div>
                            <span>
                              <button
                                type="button"
                                className="tg-btn-yellow  faqEditButtonsStyle "
                                id={faq.id}
                                onClick={e => this.editFaq(e, faq, index)}
                              >
                                Edit
                              </button>
                              <button
                                type="button"
                                className=" tg-btn-red float-right faqDeleteButtonsStyle"
                                id={faq.id}
                                onClick={e => this.deleteFaqs(e, faq, index)}
                              >
                                Delete
                              </button>
                            </span>
                          </div>
                        </UncontrolledCollapse>
                      </Card>
                    </div>
                  )}
                </Draggable>
              ))}
              {provided.placeholder}
            </div>
          )}
        </Droppable>
      </DragDropContext>
